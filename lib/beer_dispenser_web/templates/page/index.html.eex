<div class="content-wrapper">
  <section class="preamble">
    <%= if @beer != nil do %>
    <%= if length(@beer.donations) > 0 do %>
      <p>
        <small>
          Thanks for the free beer <%= donors_names(@beer.donations) %>.
          <br>
          (<strong><%= beverages_left(@beer.donations)%></strong> left)
        </small>
      </p>
      <ul style="position: relative; margin-bottom: 5rem">
        <%= for d <- donation_messages(@beer.donations) do %>
        <li style="list-style-type: none;" class="donation-quote">
          <%= d.message %> <small>- <%= d.name %></small>
        </li>
        <% end %>
      </ul>
    <% end %>

    <div class="beer-button-container">
      <h1>Thirsty?</h1>
      <%= link to: Routes.debts_path(@conn, :charge, @beer.id), method: :post, class: "beer-button #{ if get_flash(@conn, :charged) do "beer-button--filled" end}" do %>

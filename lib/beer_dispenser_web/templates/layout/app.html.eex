          </svg>

          <nav role="navigation" class="navigation">
            <%= if @current_user do %>
              <%= link "Home", to: Routes.page_path(@conn, :index) %>
              <%= link "Profil", to: Routes.user_path(@conn, :index) %>
              <%= if BeerDispenser.Accounts.User.has_role?(@current_user, "admin") do %>
                <%= link "Admin", to: Routes.admin_page_path(@conn, :index) %>
              <% end %>
              <%= link "Logout", to: Routes.session_path(@conn, :delete, current_session(@conn)), method: :delete %>
            <% else %>
              <%= link "Login", to: Routes.session_path(@conn, :new) %>
            <% end %>
          </nav>
        </section>
        <div class="divider"></div>
      </header>
      <main role="main">
        <p class="alert alert-info" role="alert"><%= get_flash(@conn, :info) %></p>
        <p class="alert alert-danger" role="alert"><%= get_flash(@conn, :error) %></p>
        <%= render @view_module, @view_template, assigns %>
      </main>
    </div>
    <%# <script type="text/javascript" src="https://www.paypal.com/sdk/js?client-id=CLIENT_ID&currency=EUR"></script> %>
    <script type="text/javascript" src="<%= Routes.static_path(@conn, "/js/app.js") %>"></script>
  </body>
</html>
